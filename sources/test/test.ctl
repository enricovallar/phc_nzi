(set! num-bands 6)
(set! resolution 32)
(set! k-points (list (vector3 0.0 0.0 0.0) (vector3 0.3333333333333333 0.0 0.0) (vector3 0.5 0.5 0.0) (vector3 0.0 0.0 0.0)))
(set! k-points (interpolate 10 k-points))
(define-param r 0.25)
(define-param r2 0.15)
(set! geometry-lattice (make lattice (size 1 1 no-size)  (basis1  (vector3 1.0 0.0 0.0)) (basis2  (vector3 0.5000000000000001 0.8660254037844388 0.0)) ))
(set! geometry (list 
 (make block (size (vector3 1e+20 1e+20 1e+20))
  (material (make dielectric (epsilon 9.61)))
  (center (vector3 0.0 0.0 0.0)))
 (make cylinder (radius r)
  (height 1e+20)
  (material (make dielectric (epsilon 1)))
  (center (vector3 0.3333333333333333 0.3333333333333333 0.0)))
 (make cylinder (radius r2)
  (height 1e+20)
  (material (make dielectric (epsilon 1)))
  (center (vector3 0.6666666666666666 0.6666666666666666 0.0)))
))
(run-te)
(run-tm)
