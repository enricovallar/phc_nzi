(set! num-bands 3)
(set! resolution (vector3 32.0 32.0 16.0))
(set! k-points (list (vector3 0.5 0.0 0.0) (vector3 0.0 0.0 0.0) (vector3 0.0 0.5 0.0)))
(set! k-points (interpolate 4 k-points))
(set! geometry-lattice (make lattice (size 1 1 4)  (basis1  (vector3 1.0 0.0 0.0)) (basis2  (vector3 0.0 1.0 0.0)) ))
(set! geometry (list (make block (size (vector3 1e+20 1e+20 0.5))
  (material (make dielectric (epsilon 9.61)))
  (center (vector3 0.0 0.0 0.0)))
(make cylinder (radius 0.2)
  (height 0.5)
  (material (make dielectric (epsilon 1)))
  (center (vector3 0.0 0.0 0.0)))
))
(run-zeven)
(run-zodd)
